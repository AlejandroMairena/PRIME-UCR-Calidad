@typeparam TEnum

<ul class="@_tabClass">
    @foreach (var tuple in Tabs)
    {
        <TabItem OnTabSetCallback="@((TEnum tab) => OnTabSet(tab))" Tab="tuple.Item1" CurrentTab="_currentTab" TabName="@tuple.Item2"/>
    }
</ul>

@code {
    protected override void OnParametersSet()
    {
        SetTabClass();
    }
}