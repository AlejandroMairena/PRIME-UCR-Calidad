@using PRIME_UCR.Components.Controls
@using Microsoft.EntityFrameworkCore

<Loading IsLoading="_values == null">
    <DropDownMenu Data="@_values" T="Pais" Label="País" TextProperty="Nombre"
            DefaultText="Seleccione un país"
            Value="_country"
            ValueExpression="() => _country"
            ValueChanged="@((Pais c) => OnCountryChange(c))"/> 
</Loading>